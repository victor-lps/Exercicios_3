programa {
  funcao inicio() {
    real numero

    escreva ("Digite um número de 0 até 10 para ser escrito por extenso: ")
    leia (numero)

    escolha (numero){
      caso 0:
      escreva ("Seu número em extenso é: Zero")
      pare
      caso 1:
      escreva("Seu número em extenso é: Um")
      pare
      caso 2:
      escreva("Seu número em extenso é: Dois")
      pare
      caso 3: 
      escreva("Seu número em extenso é: Três")
      pare
      caso 4: 
      escreva("Seu número em extenso é: Quatro")
      pare
      caso 5: 
      escreva("Seu número em extenso é: Cinco")
      pare
      caso 6: 
      escreva("Seu número em extenso é: Seis")
      pare
      caso 7:
      escreva("Seu número em extenso é: Sete")
      pare
      caso 8:
      escreva("Seu número em extenso é: Oito")
      pare
      caso 9:
      escreva("Seu número em extenso é: Nove")
      pare
      caso 10:
      escreva("Seu número em extenso é: Dez")
      pare
      
      caso contrario:
      escreva ("Número inválido!")
      retorne


    }

    

    
  }
}
